        type            zeroGradient;
        value           uniform (0 0 0);
    }
    
    outlet_side_5
    {
        type            zeroGradient;
        value           uniform (0 0 0);
    }
    
    outlet_side_6
    {
        type            zeroGradient;
        value           uniform (0 0 0);
    }
    
    outlet_side_7
    {
        type            zeroGradient;
        value           uniform (0 0 0);
    }
    
    outlet_side_8
    {
        type            zeroGradient;
        value           uniform (0 0 0);
    }
	
	workpiece
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
	
    front
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }
	
    tool
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }
    
    wall
    {
      type            fixedValue;
      value           uniform (0 0 0);
    }

   outlet
   {
       type            fixedValue;
       value           uniform (0 0 0);
   }

